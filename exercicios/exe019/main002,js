function print(a) {
  console.log(a);
}

let num = [4, 5, 6, 7, 8]

for(var i = 0; i < num.length; i++){
    print(`A ${i}º posição tem o valor ${num[i]}`)
} // Forma tradicional 

print("----------------------------------")
for(let i in num){
    print(`A ${i}º posição tem o valor ${num[i]}`)
} // Forma simplificada